     19          
     20          //-------------------------------------------------------------------------------------------------
     21          //! @brief      Returns number of ticks since startup.
     22          // Note:        The count rolls over at ~49.7 days.
     23          //!
     24          //! @retval     - true: successful.
     25          //! @retval     - false: failure
     26          //-------------------------------------------------------------------------------------------------
     27          clock_time_t SiiOsTimerTotalElapsed ( void )
     28          {
     29              return( SiiPlatformTimerSysTicksGet());
     30          }
     31          
     32          //-------------------------------------------------------------------------------------------------
     33          //! @brief      Start an OS timer.
     34          //!
     35          //! @retval     - true: successful.
     36          //! @retval     - false: failure
     37          //-------------------------------------------------------------------------------------------------
     38          void SiiOsTimerSet ( SiiTimer_t *timerInfo, clock_time_t delay  )
     39          {
     40              timerInfo->msDelay  = delay;
     41              timerInfo->msStart  = SiiOsTimerTotalElapsed();
     42              timerInfo->enabled  = true;
     43              timerInfo->oneShot  = false;
     44          }
     45          
     46          //-------------------------------------------------------------------------------------------------
     47          //! @brief      Start an OS timer.
     48          //!
     49          //! @retval     - true: successful.
     50          //! @retval     - false: failure
     51          //-------------------------------------------------------------------------------------------------
     52          void SiiOsTimerSetOneShot ( SiiTimer_t *timerInfo, clock_time_t delay  )
     53          {
     54              timerInfo->msDelay  = delay;
     55              timerInfo->msStart  = SiiOsTimerTotalElapsed();
     56              timerInfo->enabled  = true;
     57              timerInfo->oneShot  = true;
     58          }
     59          
     60          //-------------------------------------------------------------------------------------------------
     61          //! @brief      Check passed OS timer for expiration.
     62          //!
     63          //! @retval     - true: successful.
     64          //! @retval     - false: failure
     65          //-------------------------------------------------------------------------------------------------
     66          bool_t SiiOsTimerExpired ( SiiTimer_t timerInfo )
     67          {
     68              if ( !timerInfo.enabled )
     69              {
     70                  return( false );
     71              }
     72              if (SkTimeDiffMs( timerInfo.msStart, SiiOsTimerTotalElapsed()) >= timerInfo.msDelay)
     73              {
     74                  if ( timerInfo.oneShot )
     75                  {
     76                      timerInfo.enabled = false;
     77                  }
     78                  return( true );
     79              }
     80              return( false );
     81          }
     82          
     83          //-------------------------------------------------------------------------------------------------
     84          //! @brief      Cancel the specified timer. Canceling will result in calls to SiiOsTimerExpired()
     85          //!             returning false for this timer.
     86          //!
     87          //! @retval     - true: successful.
     88          //! @retval     - false: failure
     89          //-------------------------------------------------------------------------------------------------
     90          void SiiOsTimerCancel ( SiiTimer_t timerInfo )
     91          {
     92          
     93              timerInfo.enabled = false;
     94          }
