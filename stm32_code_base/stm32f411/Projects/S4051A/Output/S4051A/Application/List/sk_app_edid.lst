     21          //------------------------------------------------------------------------------
     22          //  Function:       SkAppEdidRead
     23          //  Description:    Read the selected EDID source into the passed array.
     24          //------------------------------------------------------------------------------
     25          
     26          bool_t SkAppEdidRead ( uint8_t source, uint8_t *pDest )
     27          {
     28              ddcComErr_t ddcResult;
     29              bool_t      success = true;
     30          
     31              switch (source)
     32              {
     33                  case EDID_RAM_0:
     34                  case EDID_RAM_1:
     35                  case EDID_RAM_2:
     36                  case EDID_RAM_3:
     37                  case EDID_RAM_4:
     38                  case EDID_RAM_5:
     39                      success = SiiDrvNvramSramRead( SRAM_P0 + source, pDest, 0, EDID_TABLE_LEN );
     40                      break;
     41                      /*
     42                  case EDID_RAM_VGA:
     43                      success = SiiDrvNvramSramRead( SRAM_VGA, pDest, 0, EDID_VGA_TABLE_LEN );
     44                      break;*/
     45                  case EDID_RAM_BOOT:
     46                      success = SiiDrvNvramSramRead( SRAM_BOOT, pDest, 0, EDID_DEVBOOT_LEN );
     47                      break;
     48                  case EDID_NVRAM:
     49                      success = SiiDrvNvramNonVolatileRead( NVRAM_HDMI_EDID, pDest, 0, EDID_TABLE_LEN );
     50                      break;
     51                  case EDID_NVRAM_BOOT:
     52                      success = SiiDrvNvramNonVolatileRead( NVRAM_BOOTDATA, pDest, 0, EDID_DEVBOOT_LEN );
     53                      break;
     54                      /*
     55                  case EDID_NVRAM_VGA:
     56                      success = SiiDrvNvramNonVolatileRead( NVRAM_VGA_EDID, pDest, 0, EDID_VGA_TABLE_LEN );
     57                      break;
     58          */
     59                  case EDID_EEPROM:
     60                      success = SiiPlatformEepromReadBlock( EE_EDID_OFFSET, pDest, EDID_TABLE_LEN );
     61                      break;
     62                  case EDID_EEPROM_BOOT:
     63                      success = SiiPlatformEepromReadBlock( EE_DEVBOOT_OFFSET, pDest, EDID_DEVBOOT_LEN );
     64                      break;
     65                      /*
     66                  case EDID_EEPROM_VGA:
     67                      success = SiiPlatformEepromReadBlock( EE_EDIDVGA_OFFSET, pDest, EDID_VGA_TABLE_LEN );
     68                      break;*/
     69                  case EDID_TV:
     70                      ddcResult = SiiDrvTpiDdcReadBlock( 0, 0, pDest, EDID_TABLE_LEN );
     71                      success = (ddcResult == SI_TX_DDC_ERROR_CODE_NO_ERROR);
     72                      break;
     73          //        case EDID_VGA:
     74          //            success = SkI2cReadVgaEdidBlock( 0, pDest, EDID_TABLE_LEN );
