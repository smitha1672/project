     44          bool_t SkAppArcDemo ( RC5Commands_t key )
     45          {
     46              // Start with ARC disabled for some of these menu selections
     47              if (( key >= RC5_1 ) && ( key <= RC5_5 ))
     48              {
     49                  if ( app.arcDemoEnabled )
     50                  {
     51                      SiiDrvArcConfigureArcPin( 0, 0, ARC_DISABLE_MODE);
     52                      app.arcDemoEnabled = false;
     53                  }
     54              }
     55          
     56          #if INC_ARC
     57              switch ( key )
     58              {
     59          #if (PHILIPS_REMOTE == 1)
     60                  case RC5_CC_SUBTITLE:
     61          #else
     62                  case RC5_HELP:
     63          #endif
     64                  	PrintMenu( pArcDemoMenu0 );
     65                      PrintStringOnOff( "1 - ARC TX ", (bool_t)(app.arcDemoEnabled && (app.arcDemoSelect == 0)));
     66                      PrintStringOnOff( "2 - ARC RX ", (bool_t)(app.arcDemoEnabled && (app.arcDemoSelect == 1)));
     67                      PrintMenu( pArcDemoMenu1 );
     68                      break;
     69          
     70                  case RC5_0:
     71                      app.demoMenuLevel = 0;
     72                      PrintAlways( demoMenuExit );
     73                      break;
     74          
     75                  case RC5_1:
     76                      app.arcDemoSelect = 0;
     77                      app.arcDemoEnabled = true;
     78                      SiiDrvArcConfigureArcPin(0, 0, ARC_TX_MODE);
     79                      PrintAlways( "SPDIF In -> ARC TX\n");
     80                      break;
     81          
     82                  case RC5_2:
     83                      app.arcDemoSelect = 1;
     84                      app.arcDemoEnabled = true;
     85                      SiiDrvArcConfigureArcPin(0, 0, ARC_RX_MODE);
     86                      PrintAlways( "ARC RX -> SPDIF Out\n");
     87                      break;
     88          
     89                  case RC5_3: // Disable actually happens before switch statement...
     90                      PrintAlways( "ARC Disabled\n");
     91                      break;
