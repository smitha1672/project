     14          
     15          extern ROM const remedyListElement_t EdidRemedyList[];
     16          
     17          
     18          //-------------------------------------------------------------------------------------------------
     19          //! @brief      Generates a remedy mask for a given DS EDID data.
     20          //!
     21          //! @param[in]  pEdidInfo - pointer to decoded EDID structure
     22          //!
     23          //! @return     @see TX_EDID_REMEDY_MASKS.
     24          //-------------------------------------------------------------------------------------------------
     25          
     26          uint16_t EdidTxRemedyMaskBuild(decodedEdid_t *pEdidInfo)
     27          {
     28          	uint16_t remedy = 0;
     29          	int i;
     30          	uint16_t m_id = pEdidInfo->ManufacturerId;
     31          	uint16_t p_id = pEdidInfo->ProductId;
     32          	
     33          	// search beside white models first
     34          	for (i=0; tEndOfList != EdidRemedyList[i].Type; i++)
     35          	{
     36          		if (tNoLimitation == EdidRemedyList[i].Type)
     37          			if ((EdidRemedyList[i].ManufacturerId == m_id) &&
     38          				(EdidRemedyList[i].ProductId == p_id))
     39          				goto exit;
     40          	}
     41          
     42          	// search for bad manufacturer
     43          	for (i=0; tEndOfList != EdidRemedyList[i].Type; i++)
     44          	{
     45          		if (tManufacturerLimitation == EdidRemedyList[i].Type)
     46          			if (EdidRemedyList[i].ManufacturerId == m_id)
     47          			{
     48          				remedy |= EdidRemedyList[i].RemedyMask;
     49          				break;
     50          			}
     51          	}
     52          
     53          	// search for bad products
     54          	for (i=0; tEndOfList != EdidRemedyList[i].Type; i++)
     55          	{
     56          		if (tProductLimitation == EdidRemedyList[i].Type)
     57          			if ((EdidRemedyList[i].ManufacturerId == m_id) &&
     58          				(EdidRemedyList[i].ProductId == p_id))
     59          			{
     60          				remedy |= EdidRemedyList[i].RemedyMask;
     61          				break;
     62          			}
     63          	}
     64          
     65          	// search for bad products made by a manufacturer during specified period of time;
     66          	// if manufacturer ID in the list is 0, the remedy applies to any manufacturer
     67          	for (i=0; tEndOfList != EdidRemedyList[i].Type; i++)
     68          	{
     69          		if (tLimitationOnTheseDays == EdidRemedyList[i].Type)
     70          			if ((EdidRemedyList[i].ManufacturerId == 0) || (EdidRemedyList[i].ManufacturerId == m_id))
     71          			{
     72          				uint8_t t_year_begin = (uint8_t) (EdidRemedyList[i].ProductId >> 8);
