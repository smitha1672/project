     33          void CecConfDeviceType (SiiCecDeviceTypes_t devicetype, SiiCecLogicalAddresses_t deviceLogicalAddr, bool_t isCecAsSwitch, bool_t isAudioSysWithSac)
     34          {
     35              uint16_t 	cecPa;
     36              SkAppCecInstanceSet( CEC_INSTANCE_AVR );
     37              SiiEdidTxInstanceSet(0);
     38              SkAppTxInstanceSet(0);
     39              // Can't initialize if valid physical address not assigned
     40              cecPa = SiiEdidTxCecPhysAddrGet();
     41              if (cecPa == 0xFFFF || !SiiCecInitialize( cecPa, devicetype ))
     42              {
     43                  PrintAlways( "\nERROR: CEC device type could not be set \n" );
     44                  return;
     45              }
     46              SiiCecSetDeviceLA(deviceLogicalAddr);
     47          #if INC_CEC_SWITCH
     48              if(isCecAsSwitch)
     49              {
     50                 	// Initialize CEC Switch component and enable its base service
     51                  SiiCecSwitchConfig();
     52                  if ( !SiiCecCallbackRegisterParser( SiiCecSwitchTaskProcess, false ))
     53                  {
     54                      PrintAlways( "\nERROR: CEC switch task registering failed \n" );
     55                      return;
     56                  }
     57                  if ( SiiCecSwitchSrvStart())
     58                  {
     59                      PrintAlways( "\nERROR: Launching CEC/Switch Active and Base Services \n" );
     60                  }
     61              }
     62          #endif
     63          #if INC_CEC_SAC
     64              if(isAudioSysWithSac)
     65              {
     66                  // Initialize CEC System Audio Control component and enable its base service
     67                  // Can't initialize SAC if valid physical address not assigned
     68          #if ( configSII_DEV_953x_PORTING == 1 )
     69                  SiiCecSacConfig(0); // take default format list
     70          #else
     71                  SiiCecSacConfig(0, NULL, 0); // take default format list
     72          #endif
     73                  if ( !SiiCecCallbackRegisterParser( SiiCecSacTaskProcess, false ))
     74                  {
     75                      PrintAlways( "\nERROR: CEC SAC task registering failed \n" );
     76                      return;
     77                  }
     78                  if ( SiiCecSacSrvStart())
     79                  {
     80                      PrintAlways( "\nERROR: Launching CEC/SAC Active and Base Services \n" );
     81                  }
     82              }
     83          #endif
     84          }
