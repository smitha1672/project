     22          //-------------------------------------------------------------------------------------------------
     23          //! @brief      Execute CEC commands written to the 0x6x register set
     24          //-------------------------------------------------------------------------------------------------
     25          bool_t RtpiExecuteCecCmd ( uint8_t cmdType )
     26          {
     27              bool_t  executedCmd = true;
     28              uint8_t cmdBuf0In, resultCode;
     29          #if INC_CEC_SAC
     30              uint8_t dummy;
     31              uint8_t cmdBuf0Out, cmdBuf1Out, cmdBuf2Out, cmdBuf3Out;
     32              int i;
     33          #endif
     34              uint8_t nameBuf[16];
     35          
     36              resultCode = RP_STS__NO_ERROR;
     37              cmdBuf0In = SiiPlatformI2cCbSlaveReadByte( 1, RP_A__CMD_BUF );
     38              switch ( cmdType )
     39              {
     40          #if INC_CEC_SAC
     41                  case RP_C__CAT_CMD__SAC_INITIATE:
     42                      if ( !SiiCecSacInitiate())
     43                      {
     44                          resultCode = RP_STS__ERROR_FAILED;
     45                      }
     46                      break;
     47          
     48                  case RP_C__CAT_CMD__SAC_TERMINATE:
     49                      if ( !SiiCecSacTerminate())
     50                      {
     51                          resultCode = RP_STS__ERROR_FAILED;
     52                      }
     53                      break;
     54          
     55                  case RP_C__CAT_CMD__SAC_STATUS_GET:
     56                      cmdBuf0Out = SiiCecSacIsSysAudioModeEnabled() ? 0x01 : 0x00;
     57                      cmdBuf0Out |= SiiCecSacIsMuted() ? 0x02 : 0x00;
     58                      cmdBuf1Out = SiiCecSacVolumeGet();
     59                      SiiRpForceWriteByte(RP_A__CMD_BUF, cmdBuf0Out );
     60                      SiiRpForceWriteByte(RP_A__CMD_BUF1, cmdBuf1Out );
     61          
     62                      // Create supported formats bit field 1
     63                      cmdBuf2Out = 0;
     64                      for ( i = 1; i < 9; i++ )
     65                      {
     66                          if ( SiiCecSacAudioFormatDetailsGet( i, &dummy, &dummy, &dummy ))
     67                              cmdBuf2Out |= (1 << (i - 1));
     68                      }
     69                      // Create supported formats bit field 2
     70                      cmdBuf3Out = 0;
     71                      for ( i = 9; i < 16; i++ )
     72                      {
     73                          if ( SiiCecSacAudioFormatDetailsGet( i, &dummy, &dummy, &dummy ))
     74                              cmdBuf3Out |= (1 << (i - 9));
     75                      }
     76          
     77                      SiiRpForceWriteByte( RP_A__CMD_BUF2, cmdBuf2Out );
     78                      SiiRpForceWriteByte( RP_A__CMD_BUF3, cmdBuf3Out );
     79                      break;
