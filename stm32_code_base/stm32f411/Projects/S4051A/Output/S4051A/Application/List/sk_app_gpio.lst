     35          // Function:    SkAppTaskGpio
     36          // Description: Wrapper for the GPIO Component at the application level
     37          // Parameters:  none
     38          // Returns:     none
     39          //------------------------------------------------------------------------------
     40          
     41          void SkAppTaskGpio ( uint8_t instance )
     42          {
     43              uint8_t     pinState[2];
     44              uint16_t    status;
     45              
     46              status = SiiDrvGpioStatus();    // Determine who triggered the interrupt
     47              if ( (status & SII_GPIO_PIN_0) || (instance == 1) )
     48              {
     49          		/*If it is marked, USB playback is able to working*/	
     50                  pinState[0] = SiiDrvGpioPinRead( SII_GPIO_PIN_0 );
     51                  pinState[1] = (SiiPlatformMHLCDSensePinGet( 1 )? SiiPortType_HDMI : SiiPortType_MHL);
     52                  //pinState[1] = SiiDrvGpioPinRead( SII_GPIO_PIN_1 );
     53                  if ( pinState[instance] )
     54          		{
     55          			//DEBUG_PRINT( MSG_DBG, "SkAppTaskGpio:: MHL channel 0 cable connect %s.\n", ( status & SII_GPIO_PIN_0 ) ? "true" : "false" );
     56          			pAppCbus->cbusEnable = true;   //Bug 31744 - With samsung S3 and S4 as MHL source, video out is not consistent when plug/unplug cable at source side
     57          
     58          		}
     59          		else
     60          		{
     61          			//DEBUG_PRINT( MSG_DBG, "SkAppTaskGpio:: MHL channel 1 cable connect %s.\n", ( status & SII_GPIO_PIN_1 ) ? "true" : "false" );
     62          			pAppCbus->cbusEnable = false;   //Bug 31744 - With samsung S3 and S4 as MHL source, video out is not consistent when plug/unplug cable at source side
     63          		}
     64          	}
     65          }
