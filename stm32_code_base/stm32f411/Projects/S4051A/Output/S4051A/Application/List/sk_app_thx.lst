     26          //------------------------------------------------------------------------------
     27          // Function:    SkAppThxTimerSet
     28          // Description: Elapsed timers for THX use
     29          // Parameters:  none
     30          // Returns:     none
     31          //------------------------------------------------------------------------------
     32          static void ThxTimerSet ( clock_time_t msDelay  )
     33           {
     34              pApp->msThxDelay = msDelay;
     35              pApp->msThxStart = SiiOsTimerTotalElapsed();
     36           }
     37          
     38          static bool_t ThxTimerElapsed ( void )
     39           {
     40              clock_time_t thisTime = SiiOsTimerTotalElapsed();
     41              return(( thisTime - pApp->msThxStart) >= pApp->msThxDelay);
     42           }
