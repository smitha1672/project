     38          //-------------------------------------------------------------------------------------------------
     39          //! @brief      Set pass through mode.
     40          //!
     41          //! @param[in]  isAudioPassEnabled     - Audio packets will pass through, if true
     42          //! @param[in]  isNonAudioPassEnabled  - Non-audio packets will pass through, if true
     43          //-------------------------------------------------------------------------------------------------
     44          
     45          void SiiDrvRxAudioMixPassThroughConfig(bool_t isAudioPassEnabled, bool_t isNonAudioPassEnabled)
     46          {
     47          	//SiiRegModify( REG_TPI__AUDIO_CFG, (BIT_TPI__AUDIO_CFG__MUTE | VAL_TPI__AUDIO_CFG__SPDIF | VAL_TPI__AUDIO_CFG__I2S), BIT_TPI__AUDIO_CFG__MUTE);
     48              SiiRegModify( REG_TX_AUD_MIX_CTRL1, BIT_TX_AUD_MIX_PASS_AUD_PKT |
     49                            BIT_TX_AUD_MIX_PASS_NAUD_PKTS | BIT_TX_AUD_MIX_DROP_GEN1,
     50                              (isAudioPassEnabled ? BIT_TX_AUD_MIX_PASS_AUD_PKT : 0) |
     51                              (isNonAudioPassEnabled ? BIT_TX_AUD_MIX_PASS_NAUD_PKTS : 0) |
     52                              /* SWWA: 23624 Always drop NULL packets. They may get in the way of mixer operation */
     53                              BIT_TX_AUD_MIX_DROP_GEN1);
     54          
     55              SiiRegModify( REG_TX_AUD_MIX_CTRL0, BIT_TX_AUD_MIX_DROP_AIF | BIT_TX_AUD_MIX_DROP_SPDIF | BIT_TX_AUD_MIX_DROP_CTS,
     56                              (isAudioPassEnabled ? 0: (BIT_TX_AUD_MIX_DROP_AIF | BIT_TX_AUD_MIX_DROP_SPDIF | BIT_TX_AUD_MIX_DROP_CTS)) );
     57          
     58              SiiRegModify( REG_TX_AUD_MIX_CTRL3, BIT_TX_AUD_MIX_AUD_TYPE_OVR, (isAudioPassEnabled ? 0: BIT_TX_AUD_MIX_AUD_TYPE_OVR));
     59          }
