     22          repeaterShaRequest_t 	sha_request_q[SII_NUM_PIPE]; /*buffer to save Rx SHA request*/
     23          uint8_t	req_index;
     24          bool_t	first_sha_request;		/*indicates first request, if true don't need to check HDCP status*/
     25          
     26          //-------------------------------------------------------------------------------------------------
     27          //! @brief      put SHA request to the queue
     28          //-------------------------------------------------------------------------------------------------
     29          static void ShaRequestPutInQue( repeaterShaRequest_t sReq)
     30          {
     31          	uint_t i;
     32          	/*only Rx request to be in the queue. Tx should request again if SHA busy*/
     33          	if (sReq.isTx)
     34          	{
     35          		return;
     36          	}
     37          	/*ignore same request*/
     38          	if (req_index != 0)
     39          	{
     40          		for (i = 0; i < SII_NUM_PIPE; i++)
     41          		{
     42          			if (sha_request_q[i].portNum == sReq.portNum)
     43          				return;
     44          		}
     45          	}
     46          	sha_request_q[req_index] = sReq;
     47          	DEBUG_PRINT( RPT_MSG_DBG, "SHA req put in queue %s %d q# %d\n ", sha_request_q[0].isTx ? "Tx":"Rx", sha_request_q[0].portNum, req_index);
     48          	req_index++;
     49          
     50          }
     51          
     52          //-------------------------------------------------------------------------------------------------
     53          //! @brief      get the next SHA request from the queue
     54          //-------------------------------------------------------------------------------------------------
     55          static repeaterShaRequest_t ShaRequestGetFromQue(void)
     56          {
     57          	uint_t i;
     58          	repeaterShaRequest_t 	sha_current_p;
     59          	sha_current_p = sha_request_q[0];
     60          
     61          	req_index--;
     62          
     63          	/*all move one step forward*/
     64          	for (i=0;i<req_index;i++)
     65          	{
     66          		sha_request_q[i] = sha_request_q[i+1];
     67          	}
     68          	DEBUG_PRINT( RPT_MSG_DBG, "SHA req get from queue %s %d q# %d\n ", sha_current_p.isTx ? "Tx":"Rx", sha_current_p.portNum, req_index);
     69          
     70          	return sha_current_p;
     71          }
     72          
     73          //-------------------------------------------------------------------------------------------------
     74          //! @brief      service the SHA request
     75          //-------------------------------------------------------------------------------------------------
     76          void RepeaterShaHandler( void )
     77          {
     78          	if ((req_index)&&(!SiiDrvShaInUse()))
     79          	{
     80          		DEBUG_PRINT( RPT_MSG_DBG, "queued SHA req\n ");
     81          		SiiDrvShaRequestStart(ShaRequestGetFromQue());
     82          	}
