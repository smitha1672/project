     19          //! @brief      Built-in message box user function.
     20          //! @param[in]  winRef      Return value from the SiiOsdCreateWindow function.
     21          //! @param[in]  action
     22          //! @param[in]  pArgs
     23          //! @return
     24          //-------------------------------------------------------------------------------------------------
     25          
     26          bool_t SiiOsdUserFuncBuiltIn ( int winRef, SiiOsdUserFunctionAction_t action, uint8_t *pArgs )
     27          {
     28              bool_t          result = true;
     29          
     30              switch ( action )
     31              {
     32                  case UFA_REPLACEWINDOW:
     33                  case UFA_CREATE:
     34                      break;
     35                  case UFA_CLOSE:
     36                      break;
     37                  case UFA_ROW_ENTER:
     38                  case UFA_SELECT:
     39                  case UFA_CANCEL:
     40                  default:
     41                      break;
     42              }
     43          
     44              return( result );
     45          }
