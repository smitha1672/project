     47          //-------------------------------------------------------------------------------------------------
     48          //! @brief      Display a message dialog box.
     49          //! @param[in]  windowId    ID of resource to provide window contents and properties. If 0, use
     50          //!                         standard SYSTEM_MSG properties.
     51          //! @param[in]  pTitleStr   Title text string (resource ID not allowed) or null if no title
     52          //! @param[in]  pMsgStr     Message text string or text resourceID
     53          //! @param[in]  startX      Upper left pixel horizontal coordinate of window
     54          //! @param[in]  startY      Upper left pixel vertical coordinate of window
     55          //! @param[in]  msTimeOut   Window close timeout in milliseconds after last command input to handler
     56          //! @param[in]  options     OPT_CENTER_WINDOW, OPT_CENTER_TEXT
     57          //!                         OPTW_ALLOW_ICON
     58          //! @retval                 Window reference value if successful
     59          //! @retval                 -1 if creation failed.
     60          //-------------------------------------------------------------------------------------------------
     61          int SiiOsdMessageBox( int windowId, char *pTitleStr, char *pMsgStr, int startX, int startY, int msTimeout, SiiOsdOptions_t options )
     62          {
     63              int             i, thisRowCount, thisColCount, winRef = -1;
     64              int             crlfCount, longestLine, lineLength, stringLength, displayRow, stringOffset, titleLength;
     65              SiiOsdWindow_t  *pWin;
     66          
     67              options &= ~OPT_SHOW;       // Hold off any show requests, we'll force a show at the end.
     68          
     69              thisColCount    = 0;
     70              thisRowCount    = 0;
     71              stringLength    = 0;
     72              titleLength = (pTitleStr) ? strlen( pTitleStr ) : 0;
     73              if (( windowId == 0 ) || (pMsgStr != NULL ))
     74              {
     75                  // Determine required window size, build the window, and display the text string.
     76                  crlfCount = 1;
     77                  longestLine = 0;
     78                  lineLength = 0;
     79                  stringLength = strlen( pMsgStr );
     80                  for ( i = 0; i < stringLength; i++ )
     81                  {
