     16          static uint16_t *pSysTimeCountCurMs;        //!< pointer to current time
     17          static uint16_t *pSysTimeCountPrvMs;        //!< pointer to previous time reading
     18          
     19          //-------------------------------------------------------------------------------------------------
     20          //! @brief      Updates the system time related variables.
     21          //!
     22          //!             Shall be called from TaskProcess() functions before any manipulations with CEC timers
     23          //!
     24          //! @param[in]  sysTimeCountMs    - delay time to set (in ticks)
     25          //! @param[in]  pSysTimeCntCurMs  - pointer to current time value (public member of an instance record)
     26          //! @param[in]  pSysTimeCntPrvMs  - pointer to previously recorded time value
     27          //!                                 (public member of an instance record)
     28          //-------------------------------------------------------------------------------------------------
     29          
     30          void CecSysTimeMsSet(uint16_t sysTimeCountMs, uint16_t *pSysTimeCntCurMs, uint16_t *pSysTimeCntPrvMs)
     31          {
     32              pSysTimeCountCurMs = pSysTimeCntCurMs;
     33              pSysTimeCountPrvMs = pSysTimeCntPrvMs;
     34          
     35              *pSysTimeCountPrvMs = *pSysTimeCountCurMs;    // memorize previous timer reading
     36              *pSysTimeCountCurMs = sysTimeCountMs;         // set current timer reading
     37          }
     38          
     39          
     40          //-------------------------------------------------------------------------------------------------
     41          //! @brief      Sets the time counter to given time in the future relatively
     42          //!             to provided system timer reading.
     43          //!
     44          //! @note       CAUTION! This function shall never be followed by the
     45          //!             CecTimeCounterUpdate() for the same timer in one
     46          //!             TaskProcess invocation
     47          //!
     48          //! @param[in]  delayTimeTck   - delay time to set (in ticks)
     49          //! @param[in]  pTimeCounter   - pointer to the time counter to set
     50          //-------------------------------------------------------------------------------------------------
     51          
     52          void CecTimeCounterSet(const uint16_t delayTimeTck, CecTimeCounter_t *pTimeCounter)
     53          {
     54              uint32_t totalTimeMs;
     55          
     56              totalTimeMs = (uint32_t) (*pSysTimeCountCurMs) + CEC_TIME_TCK_MS * ((uint32_t) delayTimeTck);
     57              pTimeCounter->tRov = (uint16_t)((totalTimeMs / 0x10000) & 0x3)+1;  // 0x10000 = 2^16	//Add "+1" by Elvis
     58              pTimeCounter->tRes = (uint16_t)((totalTimeMs / CEC_TIME_TCK_MS) & 0x3FFF); // mod (2^16)
     59          }
