     20          //-------------------------------------------------------------------------------------------------
     21          //! @brief      Execute CBUS commands written to the 0x6x register set
     22          //-------------------------------------------------------------------------------------------------
     23          bool_t RtpiExecuteCbusCmd ( uint8_t cmdType )
     24          {
     25              bool_t  executedCmd = true;
     26          
     27              switch ( cmdType )
     28              {
     29                  case RP_C__CAT_CMD__MHL_WRITE_MSG:
     30                      {
     31                          bool_t result = 0;
     32          
     33                          // For MHL Execute commands, register RP_A__CMD_DEVICE_ID contains the port number
     34                          SiiCbusInstanceSetBasedOnPort(SiiPlatformI2cCbSlaveReadByte(1, RP_A__CMD_DEVICE_ID));
     35                          switch (SiiPlatformI2cCbSlaveReadByte(1, RP_A__CMD_BUF))//sub command
     36                          {
     37                              case MHL_MSC_MSG_RCP:
     38                                  result = SiiMhlRxSendRCPCmd(SiiPlatformI2cCbSlaveReadByte(1, RP_A__CMD_BUF1)); //data in 0x71
     39                                  break;
     40                              case MHL_MSC_MSG_RCPK:
     41                                  result = SiiMhlRxSendRapk(SiiPlatformI2cCbSlaveReadByte(1, RP_A__CMD_BUF1)); //data in 0x71
     42                                  break;
     43                              case MHL_MSC_MSG_RCPE:
     44                                  result = SiiMhlRxSendRcpe(SiiPlatformI2cCbSlaveReadByte(1, RP_A__CMD_BUF1)); //data in 0x71
     45                                  break;
     46                          }
     47                          SiiRpForceWriteByte(RP_A__CMD_BUF2, result);
     48                      }
     49                      break;
     50                  case RP_C__CAT_CMD__MHL_READ_MSG:
     51                      {
     52                          bool_t result;
     53                          uint8_t msgCmd;
     54                          uint8_t msgData;
     55                          SiiCbusInstanceSetBasedOnPort(SiiPlatformI2cCbSlaveReadByte(1, RP_A__CMD_DEVICE_ID));
     56                          result = SiiMhlRxGetMscMsgCmd(&msgCmd, &msgData);
     57                          SiiRpForceWriteByte(RP_A__CMD_BUF, msgCmd);
     58                          SiiRpForceWriteByte(RP_A__CMD_BUF1, msgData);
     59                          SiiRpForceWriteByte(RP_A__CMD_BUF2, result);
     60                      }
     61                      break;
     62                  case RP_C__CAT_CMD__MHL_WRITE_BURST:
     63                      {
     64                          uint8_t buf[16];
     65                          uint16_t offset, length;
     66                          bool_t result;
     67                          SiiCbusInstanceSetBasedOnPort(SiiPlatformI2cCbSlaveReadByte(1, RP_A__CMD_DEVICE_ID));
     68                          SiiRpReadBlock(RP_A__CMD_BUF, buf, 16);
