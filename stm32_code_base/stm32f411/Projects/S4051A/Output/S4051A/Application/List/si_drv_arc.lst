     18          //------------------------------------------------------------------------------
     19          //  ARC Driver Instance Data
     20          //------------------------------------------------------------------------------
     21          
     22          ArcDrvInstanceData_t arcDrvInstance[SII_NUM_ARC];
     23          ArcDrvInstanceData_t *pDrvArc = &arcDrvInstance[0];
     24          
     25          //------------------------------------------------------------------------------
     26          //! @brief  Return the current instance index.
     27          //------------------------------------------------------------------------------
     28          int SiiDrvArcInstanceGet ( void )
     29          {
     30              return( pDrvArc->instanceIndex );
     31          }
     32          
     33          //------------------------------------------------------------------------------
     34          //! @brief  Set the component global data pointer to the requested instance.
     35          //! @param[in]  instanceIndex
     36          //! @return     true if legal index, false if index value is illegal.
     37          //------------------------------------------------------------------------------
     38          bool_t SiiDrvArcInstanceSet ( int_t instanceIndex )
     39          {
     40              if ( instanceIndex < SII_NUM_ARC )
     41              {
     42                  pDrvArc = &arcDrvInstance[instanceIndex];
     43                  pDrvArc->instanceIndex = instanceIndex;
     44                  return( true );
     45              }
     46          
     47              return( false );
     48          }
     49          
     50          //------------------------------------------------------------------------------
     51          //! @brief  Places the ARC driver into standby mode if available
     52          //! @return     true if the ARC driver entered standby mode successfully,
     53          //!             or false if some failure occurred.
     54          //------------------------------------------------------------------------------
     55          bool_t SiiDrvArcStandby ( void )
     56          {
     57              SiiRegModify( REG_ARC_SPDIF_CTRL, BIT_SPDIF_OUT_MODE_SEL_0, CLEAR_BITS );
     58              return( true );
     59          }
     60          
     61          //------------------------------------------------------------------------------
     62          //! @brief  Causes operation of the ARC driver to return to the state
     63          //!         it was prior to the previous call to SiiDrvArcStandby.
     64          //! @return     true if the ARC driver successfully exited standby mode,
     65          //!             or false if some failure occurred.
     66          //------------------------------------------------------------------------------
     67          bool_t SiiDrvArcResume ( void )
     68          {
     69              SiiRegModify( REG_ARC_SPDIF_CTRL, BIT_SPDIF_OUT_MODE_SEL_0, SET_BITS );
     70              return( true );
     71          }
     72          
     73          //------------------------------------------------------------------------------
     74          //! @brief  Initialize ARC driver
     75          //------------------------------------------------------------------------------
     76          bool_t SiiDrvArcInitialize ( void )
     77          {
     78              int instanceIndex;
     79          
     80              // Clear instance data
     81              instanceIndex = pDrvArc->instanceIndex;         // Remember our instance
     82              memset( pDrvArc, 0, sizeof( ArcDrvInstanceData_t ));
     83              pDrvArc->instanceIndex = instanceIndex;         // Restore our instance
     84          
     85              // Start off with ARC drivers disabled
     86              return( SiiDrvArcStandby() );
     87          }
