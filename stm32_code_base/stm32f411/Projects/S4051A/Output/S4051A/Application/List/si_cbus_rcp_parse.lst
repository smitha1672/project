    136          //------------------------------------------------------------------------------
    137          //! @brief  Check for valid CBUS RCP key code.  Although there is a one-to-one
    138          //!         correspondence for most of the key codes, some codes supported by
    139          //!         CEC are not supported by RCP and should be ignored.
    140          //! @param[in]  CEC RC key
    141          //------------------------------------------------------------------------------
    142          bool_t SiiMhlValidateRcpKeyCode ( uint8_t keyData, uint8_t supportMask )
    143           {
    144              bool_t  validKey = true;
    145          
    146              // Is it valid for the specified logical device types
    147              if (( rcpValidate[ keyData & ~BIT7] & supportMask ) == 0 )
    148              {
    149                  validKey = false;
    150              }
    151          
    152              return( validKey );
    153           }
    154          
    155          //------------------------------------------------------------------------------
    156          //! @brief  Send MSC_MSG (RCP) message to the CBUS channel.  Does not validate
    157          //!         key code because we don't want to assume anything about the
    158          //!         attached device support capabilities
    159          //! @param[in]  keyId   RCP key ID value
    160          //! @retval     true        - successful queue/write
    161          //! @retval     false       - write and/or queue failed
    162          //------------------------------------------------------------------------------
    163          bool_t SiiMhlRxSendRCPCmd ( uint8_t keyCode )
    164          {
    165              return( CBusSendMscMsgCmd( MHL_MSC_MSG_RCP, keyCode ));
    166          }
