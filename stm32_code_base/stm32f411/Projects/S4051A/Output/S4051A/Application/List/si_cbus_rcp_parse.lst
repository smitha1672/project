    168          //------------------------------------------------------------------------------
    169          //! @brief  Transfer a CBUS message to CEC
    170          //! @param[in]
    171          //------------------------------------------------------------------------------
    172          void CbusProcessRcpRap ( uint8_t cmd, uint8_t msgData )
    173          {
    174              uint8_t     status;
    175              switch ( cmd )
    176              {
    177                  case MHL_MSC_MSG_RCP:
    178                      DEBUG_PRINT( MSG_DBG, "RCP received. Key Code: %02X\n", msgData );
    179          
    180                      // Don't bother application with basic key code validation, do it here.
    181                      if ( !SiiMhlValidateRcpKeyCode( msgData, MHL_LD_ALL ))
    182                      {
    183                          SiiMhlRxSendRcpe( MHL_MSC_MSG_RCP_INEFFECTIVE_KEY_CODE );
    184                      }
    185                      else
    186                      {
    187                          // Allow application to process key.
    188                          status = SiiMhlCbRxRcpRapReceived( cmd, msgData );
    189                          if ( status != MHL_MSC_MSG_RCP_NO_ERROR )
    190                          {
    191                              SiiMhlRxSendRcpe( status );
    192                          }
    193                      }
    194          
    195                      // Acknowledge command whether there was an error or not.
    196                      SiiMhlRxSendRcpk( msgData );
    197                      break;
    198                  case MHL_MSC_MSG_RAP:
    199                  	DEBUG_PRINT( MSG_DBG, "RAP received. Action Code: %02X\n", msgData );
    200                      status = SiiMhlCbRxRcpRapReceived( cmd, msgData );
    201                      SiiMhlRxSendRapk( status );
    202                     break;
    203          #if ((MHL_20 == ENABLE) || (MHL_EXTRA == ENABLE))
    204                  case MHL_MSC_MSG_UCP:
    205                      DEBUG_PRINT( MSG_DBG, "UCP received. Key Code: %02X\n", msgData );
    206                      // Allow application to process key.
    207                      status = SiiMhlCbRxRcpRapReceived( cmd, msgData );
    208                      if ( status != MHL_MSC_MSG_UCP_NO_ERROR )
    209                      {
    210                          SiiMhlRxSendUcpe( status );
    211                      }
    212                      SiiMhlRxSendUcpk( msgData );
    213                      break;
    214          #endif
    215          
    216                  case MHL_MSC_MSG_RCPK:
    217                  case MHL_MSC_MSG_RCPE:
    218                  case MHL_MSC_MSG_RAPK:
    219                  case MHL_MSC_MSG_UCPK:
    220                  case MHL_MSC_MSG_UCPE:
    221                      status = SiiMhlCbRxRcpRapReceived( cmd, msgData );
    222                      break;
    223          
    224                  default:
    225                      DEBUG_PRINT( MSG_DBG, "MSC_MSG sub-command not recognized!! Sending back MSGE code !!\n" );
    226                      SiiMhlRxSendMsge();
    227                      break;
    228              }
    229          }

