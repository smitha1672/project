     21          //------------------------------------------------------------------------------
     22          // Function:    DemoCdcInfoPrint
     23          // Description: Show current CDC status
     24          //------------------------------------------------------------------------------
     25          
     26          static void DemoCdcInfoPrint ( void )
     27          {
     28              int i;
     29              char *pStateStr;
     30              CdcHecDescriptor_t  *pHec            = SiiCdcHecRecordsGet();
     31              CdcPhysAddrTime_t   *pAliveDevice    = SiiCdcAliveDeviceListGet();
     32              CdcFoundListEntry_t *pFoundHostsList = SiiCdcFoundHostListGet();
     33          
     34              // show HEC list
     35              DEBUG_PRINT( MSG_STAT, "\n-------- HEC List --------\n" );
     36              for (i = 0; i < CDC_NUMBER_OF_HECS_MAX; i++)
     37              {
     38                  if (pHec[i].status.isInUse == true)
     39                  {
     40                      switch (pHec[i].status.channelState)
     41                      {
     42                          default:
     43                          case CDC_PHEC:
     44                              pStateStr = "PHEC";
     45                              break;
     46                          case CDC_VHEC:
     47                              pStateStr = "VHEC";
     48                              break;
     49                          case CDC_AHEC:
     50                              pStateStr = "AHEC";
     51                              break;
     52                      }
     53          
     54                      DEBUG_PRINT(
     55                          MSG_STAT,
     56                          "%04x >> %04x, act: %04x, typ: %s, state: %s\n",
     57                          pHec[i].firstDevPhysAddr,
     58                          pHec[i].lastDevPhysAddr,
     59                          pHec[i].activatorPhysAddr,
     60                          pHec[i].status.isAdjacent ? "adjacent" : "normal",
     61                          pStateStr );
     62                  }
     63              }
     64          
     65              // show alive list
     66              DEBUG_PRINT( MSG_STAT, "-------- Alive List --------\n" );
     67              for (i = 0; i < CDC_NUMBER_OF_DEVICES_MAX; i++)
     68              {
     69                  if (pAliveDevice[i].physAddr != 0xFFFF)
     70                  {
     71                      DEBUG_PRINT(
     72                          MSG_STAT,
     73                          "%04x, left: %d s\n",
     74                          pAliveDevice[i].physAddr,
     75                          CdcTimeCounterLeftSecGet(&pAliveDevice[i].TimeCount)
     76                          );
     77                  }
     78          
     79              }
     80          
     81              // show found devices list
     82              DEBUG_PRINT( MSG_STAT, "-------- Found Devices List --------\n" );
     83          
     84              for (i = 0; i < SiiCdcNumberOfFoundHostsGet(); i++)
     85              {
     86                  DEBUG_PRINT( MSG_STAT, "%04x [h = %d, e = %d, sel = %d]\n",
     87                      pFoundHostsList[i].physAddr,
     88                      (int) pFoundHostsList[i].devStat.hostFuncState,
