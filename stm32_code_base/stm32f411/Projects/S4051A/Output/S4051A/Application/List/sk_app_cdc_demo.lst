    126          // Function:    DemoCdcSingleDeviceConnectTest
    127          // Description: Activates and Deactivates HECs to the devices from
    128          //              the device found list
    129          //------------------------------------------------------------------------------
    130          
    131          static void DemoCdcSingleDeviceConnectTest ( uint8_t devIndex )
    132          {
    133              CdcFoundListEntry_t *pFoundHostsList = SiiCdcFoundHostListGet();
    134              uint16_t             physAddr        = SiiCdcPhysAddrGet();
    135          
    136              if (CdcHecIndexGet(physAddr, physAddr, pFoundHostsList[devIndex].physAddr) == 0xFF)
    137              {
    138                  SiiCdcDeviceConnect( false, physAddr, pFoundHostsList[devIndex].physAddr);
    139                  DEBUG_PRINT(
    140                      MSG_STAT,
    141                      "TEST: Activation of a HEC %d [%04x,%04x]\n",
    142                      (int)devIndex,
    143                      physAddr,
    144                      pFoundHostsList[devIndex].physAddr );
    145              }
    146              else
    147              {
    148                  SiiCdcDeviceDisconnect( false, physAddr, physAddr, pFoundHostsList[devIndex].physAddr);
    149                  DEBUG_PRINT(
    150                      MSG_STAT,
    151                      "TEST: Deactivation of a HEC %d [%04x,%04x]\n",
    152                      (int) devIndex,
    153                      physAddr,
    154                      pFoundHostsList[devIndex].physAddr );
    155              }
    156          }
