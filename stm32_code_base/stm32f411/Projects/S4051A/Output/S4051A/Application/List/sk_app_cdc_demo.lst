    100          static void DemoCdcInquireStateTest ( void )
    101          {
    102              uint8_t hecIdx;
    103              CdcHecDescriptor_t  *pHec     = SiiCdcHecRecordsGet();
    104              uint16_t             physAddr = SiiCdcPhysAddrGet();
    105          
    106              hecIdx = CdcHecDescriptorAdd(true);
    107          
    108              pHec[hecIdx].activatorPhysAddr = physAddr;
    109              pHec[hecIdx].firstDevPhysAddr  = physAddr;
    110              pHec[hecIdx].lastDevPhysAddr  = 0x2000; // TE ID1 (HEACT Table 7-1)
    111          
    112              CdcHecInquireStateSend(hecIdx);
