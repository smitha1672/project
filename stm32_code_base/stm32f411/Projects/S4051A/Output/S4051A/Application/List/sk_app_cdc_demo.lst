    115                  MSG_STAT,
    116                  "InquireState sent to HEC %d [%04x,%04x]\n",
    117                  (int)hecIdx,
    118                  pHec[hecIdx].firstDevPhysAddr,
    119                  pHec[hecIdx].lastDevPhysAddr );
    120          
    121              CdcHecDescriptorRemove(hecIdx);
    122          }
