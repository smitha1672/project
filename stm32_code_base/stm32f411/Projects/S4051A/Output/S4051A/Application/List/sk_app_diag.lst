     28          //-------------------------------------------------------------------------------------------------
     29          //! @brief      Display major state indications of the Switch
     30          //!
     31          //-------------------------------------------------------------------------------------------------
     32          
     33          static void DumpSwitchState ( void )
     34          {
     35              int_t       i;
     36              bool_t      first = true;
     37              uint8_t     uData, uData1, mpPortBitMask, portIndex;
     38              uint16_t    uData16;
     39              uint8_t     pwr5vStatus, hDdc, eDdc, ckdtStatus;
     40              uint16_t    termCtrl, bcaps;
     41              // Overall state
     42          
     43              PrintAlways( "\n---- Switch (Rx) State ----\n" );
     44              uData = SiiRegRead( REG_MP_STATE );
     45              DEBUG_PRINT( MSG_ALWAYS, " PWD Power: %s\n", (uData & BIT_PWRON_STANDBY) ? "ON" : "STANDBY" );
     46              uData = SiiRegRead( REG_TMDST_CTRL1 );
     47              DEBUG_PRINT( MSG_ALWAYS, "       Tx0: %s (%02X)\n",
     48                  ((uData & (MSK_TMDS_OE_ALL | MSK_TMDS_EN)) == (MSK_TMDS_OE_ALL | MSK_TMDS_EN)) ? "Enabled" : "---", uData );
     49              uData = SiiRegRead( REG_TMDST_CTRL3 );
     50              DEBUG_PRINT( MSG_ALWAYS, "  Tx0 Term: %s (%02X)\n",
     51                  ((uData & MSK_TMDS_TERM_EN_ALL) == MSK_TMDS_TERM_EN_ALL) ? "Enabled" : "---", uData );
     52              uData = SiiRegRead( REG_MISC_CTRL1 );
     53              DEBUG_PRINT( MSG_ALWAYS, "  Port Change: %s (%02X)\n", (uData & BIT_PORT_CHG_ENABLE) ? "Enabled" : "Disabled", uData );
     54              PrintAlways( "\n" );
     55              uData = (SiiRegRead( REG_PHYS_HPD_DISABLE ) >> 2) & 0x1F;
     56              PrintAlways( "HEC/MHL ports: " );
     57              for ( i = 0; i < SII_INPUT_PORT_COUNT; i++ )
     58              {
     59                  if ( uData & ( 1 << i))
     60                  {
     61                      if ( !first )
     62                      {
     63                          PrintAlways( ", " );
     64                      }
     65                      DEBUG_PRINT( MSG_ALWAYS, "%d", i );
     66                      first = false;
     67                  }
     68              }
     69              DEBUG_PRINT( MSG_ALWAYS, " (Bits: 0x%02X)\n\n", uData );
     70          
     71              // Main Pipe State
     72          
     73              PrintAlways( "----     Main Pipe     ----\n" );
     74              uData = SiiRegRead( REG_SELECTED_PORT_0 ) & MAIN_PIPE_MASK;
     75              mpPortBitMask = uData;
     76              uData = SiiRegRead( REG_RX_PORT_SEL );
