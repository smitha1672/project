     31          //------------------------------------------------------------------------------
     32          // Function:    SkAppCdcMsgHandler
     33          // Description: Monitor CDC sub-system.  Called from the CEC handler
     34          //------------------------------------------------------------------------------
     35          
     36          bool_t SkAppCdcMsgHandler ( SiiCpiData_t *pMsg )
     37          {
     38              bool_t  usedMessage;
     39              uint8_t fbMsg;
     40              int i;
     41              uint16_t timeTickMs;
     42              CdcFoundListEntry_t *foundDevList;
     43          
     44              // Bring RX CEC into current CPI context. HEC is available on SiI9489 RX ports only.
     45              // Note: Not using of SiiCdcInstanceSet() here is essential: CEC component has bigger set of
     46              // options (CEC RX & CEC TX), so the CDC can use either of them still being instantiated
     47              // only once.
     48              SkAppCecInstanceSet( CEC_INSTANCE_AVR );
     49          
     50              // Call the main CDC handler. We pass it the system tick timer's value
     51              // so that it can update its internal timers.
     52          
     53              timeTickMs = SiiPlatformTimerElapsed( ELAPSED_TIMER1 );
     54              fbMsg = SiiCdcTaskProcess( timeTickMs, pMsg );
     55          
     56              /* Process feedback messages.       */
     57          
     58              usedMessage = true;
     59              switch ( fbMsg )
     60              {
     61                  default:
     62                  case CDC_FB_MSG_NONE:
     63                      // nothing to do
     64                      break;
     65                  case CDC_FB_MSG_HST_SEARCH_DONE:    // Host search done, print list of found devices.
     66                      foundDevList =  SiiCdcFoundHostListGet();
     67                      for (i = 0; i < SiiCdcNumberOfFoundHostsGet(); i++)
     68                      {
     69                          DEBUG_PRINT( MSG_STAT, "%04x", foundDevList[i].physAddr );
     70                          if ( foundDevList[i].hostInfo.isSelected )
     71                          {
     72                              DEBUG_PRINT( MSG_STAT, " +" );
     73                          }
     74                          DEBUG_PRINT( MSG_STAT, "\n" );
     75                      }
     76                      lastFbMsg = fbMsg;
     77                      break;
     78                  case CDC_FB_MSG_CONNECT_DONE:       // Update lists
     79                      DEBUG_PRINT( MSG_STAT, "Connection task finished\n" );
     80                      lastFbMsg = fbMsg;
     81                      break;
     82                  case CDC_FB_MSG_DISCONNECT_DONE:
     83                  case CDC_FB_MSG_DISCONNECT_ALL_DONE:
     84                  case CDC_FB_MSG_DISCONNECT_LOST_DONE:
     85                      DEBUG_PRINT( MSG_STAT, "Disconnect task finished\n" );
     86                      break;
     87                  case CDC_FB_MSG_HPD_SIGNAL_DONE:
