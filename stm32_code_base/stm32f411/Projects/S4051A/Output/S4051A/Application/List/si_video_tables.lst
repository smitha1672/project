    211          //-------------------------------------------------------------------------------------------------
    212          //! @brief      Return pixel repetition factor.
    213          //!
    214          //! @param[in]  videoIndex - index of video mode listed in the VideoModeTable
    215          //-------------------------------------------------------------------------------------------------
    216          
    217          uint8_t SiiVideoTableRepetitionGet(uint8_t videoIndex)
    218          {
    219          	uint8_t result = 0;
    220          
    221          	// Note: no PC modes have repetition
    222          
    223          	if(videoIndex != SI_VIDEO_MODE_NON_STD)
    224          	{
    225          		uint8_t repetition = VideoModeTable[videoIndex].Repetition;
    226          
    227          		// chose smallest allowed replication
    228          		if(repetition & RP1)
    229          			result = 0;
    230          		else if(repetition & RP2)
    231          			result = 1;
    232          		else if(repetition & RP4)
    233          			result = 3;
    234          		else if(repetition & RP5)
    235          			result = 4;
    236          		else if(repetition & RP7)
    237          			result = 6;
    238          		else if(repetition & RP8)
    239          			result = 7;
    240          		else if(repetition & RP10)
    241          			result = 9;
    242          	}
    243          	
    244          	return result;
    245          }
