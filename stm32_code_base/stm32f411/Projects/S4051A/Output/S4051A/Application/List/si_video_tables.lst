    248          //-------------------------------------------------------------------------------------------------
    249          //! @brief      Finds a first match in the video table.
    250          //!
    251          //! @param[in]  resolution - active pixel size of the frame
    252          //! @param[in]  interlaced - interlacing flag
    253          //!
    254          //! @return     pointer to a video mode descriptor in Video Table, 0 if no match found
    255          //-------------------------------------------------------------------------------------------------
    256          
    257          const videoMode_t *SiiVideoTableMatchFind(uint16_t resH, uint16_t bkH, uint16_t resV, uint16_t bkV, bool_t interlaced)
    258          {
    259              uint_t i;
    260              const videoMode_t *pVideoMode = 0;
    261          
    262              for (i = 0; i < NMB_OF_VIDEO_MODES; i++)
    263              {
    264                  if ( (VideoModeTable[i].Active.H == resH) &&
    265                       (VideoModeTable[i].Blank.H == bkH) &&
    266                       (VideoModeTable[i].Active.V == resV) &&
    267                       (VideoModeTable[i].Blank.V == bkV) &&
    268                       (VideoModeTable[i].Interlaced == interlaced))
    269                  {
    270                      pVideoMode = &VideoModeTable[i];
    271                      break;
    272                  }
    273              }
    274              return pVideoMode;
    275          }
