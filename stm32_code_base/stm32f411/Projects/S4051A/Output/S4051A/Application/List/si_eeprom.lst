     18          //------------------------------------------------------------------------------
     19          // Function:    SiiPlatformEepromWriteBlock
     20          // Description:	Write a block of data to the EEPROM
     21          // Parameters:  addr
     22          // Returns:     true if successful, false if an error occurred
     23          //
     24          // Note:		The Microchip 24LC128 uses an internal 64-byte
     25          //              page buffer, and block writes must be aligned.
     26          // Note:		A write is not complete until the EE_PROM begins ACKing
     27          //				again.
     28          // Note:		PollForAck cannot be done on Stellaris I2C controller
     29          // Note:		The SiiPlatformI2cWriteBlock() function updates lastResultCode and i2CError
     30          //------------------------------------------------------------------------------
     31          
     32          bool_t SiiPlatformEepromWriteBlock ( uint16_t addr, const uint8_t *pWrData, uint16_t nBytes )
     33          {
     34              bool_t        success = true;
     35              
     36          #if defined ( SII_DEV_953x_PORTING_PLATFORM_EEPROM )
     37              uint16_t    i, packetLen;
     38          
     39              if ( addr & (64-1))
     40              {
     41                  packetLen = addr & (64-1);
     42                  if ( packetLen > nBytes )
     43                      packetLen = nBytes;
     44          
     45                  // Write individual bytes until we get to a 64 byte boundary
     46                  // or run out of bytes.
     47          
     48                  for ( i = 0; (( i < packetLen ) && success ); i++ )
     49                  {
     50                      success = SiiPlatformI2cWriteBlock16( 0, EEPROM_I2C_ADDRESS, addr, pWrData, 1 );
     51                      if ( success )
     52                      {
     53                          success = SiiPlatformI2cPollForAck ( 0, EEPROM_I2C_ADDRESS );
     54                      }
     55                      addr++;
     56                      pWrData++;
     57                  }
     58                  nBytes -= packetLen;
     59              }
     60          
     61              // Write the remaining aligned packets.
     62          
     63              for ( i = 0; (( i < nBytes ) && success ); i++ )
     64              {
     65                  packetLen = 64;
     66                  if ( packetLen > nBytes )
     67                      packetLen = nBytes;
     68                  success = SiiPlatformI2cWriteBlock16( 0, EEPROM_I2C_ADDRESS, addr, pWrData, packetLen );
     69                  if ( success )
     70                  {
     71                      success = SiiPlatformI2cPollForAck ( 0, EEPROM_I2C_ADDRESS );
     72                  }
     73                  addr    += packetLen;
     74                  pWrData += packetLen;
     75                  nBytes  -= packetLen;
     76              }
     77          #endif 
     78          
     79              return( success );
     80          }
     81          
     82          //------------------------------------------------------------------------------
     83          // Function:    SiiPlatformEepromWriteByte
     84          // Description:	Write a single byte of data to the on-board EEPROM
     85          // Parameters:  addr
     86          //				value
     87          // Returns:
     88          // Note:		The SiiPlatformEepromWriteBlock() function updates lastResultCode and i2CError
