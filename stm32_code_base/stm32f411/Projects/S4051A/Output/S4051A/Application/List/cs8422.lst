    116          static void CS8422_ForcesClockControl( uint8 index )
    117          {
    118              uint8 ret = 0;
    119          
    120              switch( index )
    121              {
    122                  case 0: /*Clock signal on XTI is output on RMCK according to the SWCLK bit functionality. */
    123                  {
    124                      ret = CS8422_ReadI2C_Byte(CS8422_CLOCK_CONTROL);
    125                      ret &= 0xBF; 
    126          
    127                      ret |=0x00;
    128                      CS8422_WriteI2C_Byte( CS8422_CLOCK_CONTROL, ret );
    129                      
    130                  }
    131                      break;
    132          
    133                  case 1:    /*Forces the clock signal on XTI to be output on RMCK regardless of the SWCLK bit functionality.*/
    134                  {
    135                      ret = CS8422_ReadI2C_Byte(CS8422_CLOCK_CONTROL);
    136                      ret &= 0xBF; 
    137          
    138                      ret |=0x40;
    139                      CS8422_WriteI2C_Byte( CS8422_CLOCK_CONTROL, ret );
    140                  }
    141                      break;
    142              }
    143          }
