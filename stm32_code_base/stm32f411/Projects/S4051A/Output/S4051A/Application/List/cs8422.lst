     87          static void CS8422_SerialAudioInput_SAI_MCLK_Control( uint8 index )
     88          {
     89              uint8 ret = 0;
     90          
     91              ret = CS8422_ReadI2C_Byte(CS8422_SAI_CLOCK_CONTROL);
     92          
     93              switch( index )
     94              {
     95                  case 0: /*XTI - XTO*/
     96                  {
     97                      ret &= 0xF0;
     98          
     99                      ret |= 0x00;
    100                      CS8422_WriteI2C_Byte(CS8422_SAI_CLOCK_CONTROL, ret);
    101                      
    102                  }
    103                      break;
    104                      
    105                  case 1:    /*RMCK*/
    106                  {
    107                      ret &= 0xF0;
    108          
    109                      ret |= 0x08;
    110                      CS8422_WriteI2C_Byte(CS8422_SAI_CLOCK_CONTROL, ret);
    111                  }
    112                      break;
    113              }
    114          }
