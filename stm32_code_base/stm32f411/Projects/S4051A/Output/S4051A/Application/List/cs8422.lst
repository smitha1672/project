    508          void CS8422_SoftReboot(void)
    509          {
    510              uint8 ret = 0;
    511              uint8 ret1 = 0;
    512              uint8 ret2 = 0;
    513              uint8 ret3 = 0;
    514          
    515              //Load current settings
    516              ret1 = CS8422_ReadI2C_Byte(CS8422_RECEIVER_INPUT_CONTROL);
    517              ret2 = CS8422_ReadI2C_Byte(CS8422_DATA_ROUNTING_CONTROL);
    518              ret3 = CS8422_ReadI2C_Byte(CS8422_SAI_CLOCK_CONTROL);
    519          
    520              //CS8422 Off
    521              ret = CS8422_ReadI2C_Byte(CS8422_CLOCK_CONTROL);
    522              ret &=0x7f;
    523              CS8422_WriteI2C_Byte( CS8422_CLOCK_CONTROL, ret );
    524          
    525          #if defined ( FREE_RTOS )    
    526              vTaskDelay(TASK_MSEC2TICKS(2));
    527          #else
    528              VirtualTimer_sleep(2);
    529          #endif 
    530          
    531          
    532              //CS8422 On
    533              ret = CS8422_ReadI2C_Byte(CS8422_CLOCK_CONTROL);
    534              ret |=0x80;
    535              CS8422_WriteI2C_Byte( CS8422_CLOCK_CONTROL, ret );
    536              
    537          #if defined ( FREE_RTOS )    
    538              vTaskDelay(TASK_MSEC2TICKS(2));
    539          #else
    540              VirtualTimer_sleep(2);
    541          #endif 
    542              
    543              CS8422_init(); 
    544          
    545              //Set the preloaded settings
    546              CS8422_WriteI2C_Byte(CS8422_RECEIVER_INPUT_CONTROL, ret1 );
    547              CS8422_WriteI2C_Byte(CS8422_DATA_ROUNTING_CONTROL, ret2 );
    548              CS8422_WriteI2C_Byte(CS8422_SAI_CLOCK_CONTROL, ret3);
    549          
    550              TRACE_DEBUG((0, "CS8422 initial!! CS8422_SPDIF_In_To_SPDIF_And_PCM_Out"));
    551          }
