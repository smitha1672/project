    203                  /*
    204                      01 - RMCK is an output and is derived from the ISCLK input frequency divided by 64. Only valid if serial
    205                      audio input port is in slave mode (SIMS = 0 in ¡§Serial Audio Input Data Format (0Bh)¡¨ on page 53).
    206                  */
    207                  case 1:
    208                  {
    209                      ret &= 0xE7; 
    210          
    211                      ret |= 0x08;
    212                      CS8422_WriteI2C_Byte( CS8422_CLOCK_CONTROL, ret );
    213                  }
    214                      break;
    215          
    216                  /*10 - RMCK is high-impedance.*/
    217                  case 2:
    218                  {
    219                      /*TBD*/
    220                  }
    221                      break;
    222              }
