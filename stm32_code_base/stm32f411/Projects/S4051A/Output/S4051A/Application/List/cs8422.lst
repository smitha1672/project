    435          void CS8422_init(void)
    436          {
    437               // Here is for crystal 12.288 Mhz @{
    438              CS8422_WriteI2C_Byte(0x00,0x00);
    439              CS8422_WriteI2C_Byte(0x01,0x10);
    440          
    441          /*    Smith comment: 2013/3/15
    442              Clock Control (02h)
    443              FSWCLK[6] ¡V Forces the clock signal on XTI to be output on RMCK regardless of the SWCLK bit functionality or PLL lock.
    444                  0 ¡V Clock signal on XTI is output on RMCK according to the SWCLK bit functionality.
    445                  1 ¡V Forces the clock signal on XTI to be output on RMCK regardless of the SWCLK bit functionality.
    446                  
    447              SWCLK[5] - Outputs XTI clock signal on RMCK pin when PLL loses lock. Any OSCLK or OLRCK derived from RMCK under normal conditions will be derived from XTI in this case. 
    448                  0 - Disable automatic clock switching. 
    449                  1 - Enable automatic clock switching on PLL unlock. Clock signal selected on XTI is automatically output on RMCK on PLL Unlock.
    450          
    451                 INT[1:0] - Interrupt output pin (INT) control
    452                 10 - Open drain, active low. Requires an external pull-up resistor on the INT pin.    
    453          */    
    454              //CS8422_WriteI2C_Byte(0x02,0x64); 
    455              CS8422_WriteI2C_Byte(0x02,0x62); 
    456          
    457              /*Receiver Input Control*/
    458              CS8422_WriteI2C_Byte(0x03,0x80);
    459              CS8422_WriteI2C_Byte(0x04,0x04); 
    460          
    461              /*CS8422 GPO0 is 96KHZ indicator; GPO1 is receiver error indicator*/
    462              //CS8422_WriteI2C_Byte(0x05,0x96);    
    463              CS8422_WriteI2C_Byte(0x05,0x90);    
    464          
    465              /*GPO2 is for interrupt pin*/
    466              //CS8422_WriteI2C_Byte(0x06,0x3E);
    467              CS8422_WriteI2C_Byte(0x06,0x30);
    468          
    469              
    470              /* Serial Audio Input Clock Control*/
    471              CS8422_WriteI2C_Byte(0x07,0x48);
    472          
    473              /*SAO_MCLK[3] Selects the MCLK as XTI/O*/
    474              CS8422_WriteI2C_Byte(0x08,0x40);
    475          
    476              /* Recevered MasterClock Ration Control */
    477              CS8422_WriteI2C_Byte(0x09,0x48);
    478          
    479              /*Data Routing Control: SRCD[1] 0- Serial Audio Input port(SDIN); 1-AES3 Receiver Output*/
    480              CS8422_WriteI2C_Byte(0x0A,0x52);    
    481          
    482              /*Serial Audio Input Data Format as 48Khz*/
    483              CS8422_WriteI2C_Byte(0x0B,0x88);
    484          
    485              /*SDOUT1 - Master mode; I2S output*/
    486              CS8422_WriteI2C_Byte(0x0C,0x84);
    487          
    488              /*SDOUT2*/
    489              CS8422_WriteI2C_Byte(0x0D,0x80);
    490          
    491              /*Receiver Error Unmasking*/
    492              CS8422_WriteI2C_Byte(0x0E,0xFF);
    493          
    494              /*INTERRUPT UNMASK*/
    495              CS8422_WriteI2C_Byte(0x0F, 0x0A);
    496          
    497              /*Set ERROR is Falling edge*/
    498              CS8422_WriteI2C_Byte(0x10,0x05);
    499              
    500              CS8422_WriteI2C_Byte(0x16,0x10);
    501          }
