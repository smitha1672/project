    428          static void CS8422_SRC_setPath_SerialAudioInput(void)
    429          {
    430              CS8422_ClockControl_RMCK( 1 ); /*fix analog source outputs "PO PO" noise*/
    431              CS8422_SerialAudioInput_SAI_MCLK_Control( 0 );    /*fix If optical doesn't connect to DVD player, analog source will lose audio.*/
    432              CS8422_SerialAudioOutput( 2 );    /*Set serial data output */
    433          }
