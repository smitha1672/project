    404          static void CS8422_SRC_setPath_Rx( uint8 rx_path, uint8 src_ctrl )
    405          {
    406              if ( rx_path > 3 )
    407                  return;
    408          
    409              if ( src_ctrl > 1 )
    410                  return;
    411          
    412              CS8422_ClockControl_RMCK( 0 );
    413              CS8422_ReceiverInputControl( rx_path );
    414              CS8422_SerialAudioInput_SAI_MCLK_Control( 1 );
    415          
    416              if ( src_ctrl == 1 ) /* SRC enable */
    417              {
    418                  CS8422_ForcesClockControl( 1 );
    419                  CS8422_SerialAudioOutput( 0 );
    420              }
    421              else
    422              {   
    423                  CS8422_ForcesClockControl( 0 );
    424                  CS8422_SerialAudioOutput( 1 );
    425              }
    426          }
