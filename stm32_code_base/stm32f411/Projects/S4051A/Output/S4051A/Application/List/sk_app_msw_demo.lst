     22          void SkAppMswDemoSetSources ( int_t portZone1, int_t portZone2 )
     23          {
     24              SiiDrvSwitchZoneTrackingSet(portZone1 == portZone2);
     25              app.newSource[0] = portZone1;
     26              app.newSource[1] = portZone2;
