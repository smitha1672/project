     39          /* ****************************************************************************  
     40           * esint8 efs_init(EmbeddedFileSystem * efs,eint8* opts)
     41           * Description: This function initialises all subelements of a filesystem.
     42           * It sets the pointerchain and verifies each step.
     43           * Return value: 0 on success and -1 on failure.
     44          */
     45          esint8 efs_init(EmbeddedFileSystem * efs,eint8* opts)
     46          {
     47          	if(if_initInterface(&efs->myCard, opts)==0)
     48          	{
     49          		ioman_init(&efs->myIOman,&efs->myCard,0);
     50          		disc_initDisc(&efs->myDisc, &efs->myIOman);
     51          		part_initPartition(&efs->myPart, &efs->myDisc);
     52          		if(efs->myPart.activePartition==-1){
     53          			efs->myDisc.partitions[0].type=0x0B;
     54          			efs->myDisc.partitions[0].LBA_begin=0;
     55          			efs->myDisc.partitions[0].numSectors=efs->myCard.sectorCount;	
     56          			/*efs->myPart.activePartition = 0;*/
     57          			/*efs->myPart.disc = &(efs->myDisc);*/
     58          			part_initPartition(&efs->myPart, &efs->myDisc);
     59          		}
     60          		/*part_initPartition(&efs->myPart, &efs->myDisc);*/
     61          		if(fs_initFs(&efs->myFs, &efs->myPart))
     62          			return(-2);
     63          		return(0);
     64          	}
     65          	return(-1);
     66          }
