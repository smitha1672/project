     73          //-------------------------------------------------------------------------------------------------
     74          //! @brief      Set video test pattern to be generated.
     75          //!
     76          //! @param[in]  pattern - pattern ID. @see tpgVideoPattern_t.
     77          //-------------------------------------------------------------------------------------------------
     78          
     79          void SiiDrvExtTpgPatternSet(tpgVideoPattern_t pattern)
     80          {
     81              pattern <<= VAL_VPG_PATTERN_SHIFT;
     82              SiiRegModify( REG_VPG_CTRL_2, MSK_VPG_PATTERN, pattern );
     83          }
