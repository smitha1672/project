     22          //-------------------------------------------------------------------------------------------------
     23          //! @brief      Set format (resolution and frame rate) of generated video.
     24          //!
     25          //! @param[in]  format - format ID. @see tpgVideoFormat_t.
     26          //! @param[in]  clockSrc  - video clock source.
     27          //-------------------------------------------------------------------------------------------------
     28          
     29          void SiiDrvTpgVideoFormatSet(tpgVideoFormat_t format, tpgClockSource_t clockSrc)
     30          {
     31          
     32              // SWWA:22565
     33              // Main pipe must be set to forced port selection mode and the selected port has to have active connection.
     34              // This prevents APLL1 from being reset by PA events.
     35              if (clockSrc == SI_TPG_CLK_P0)
     36              {
     37                  uint8_t ckdt;
     38                  int_t   port = 1;
     39          
     40                  ckdt = SiiRegRead(REG_CLKDETECT_STATUS) & MSK_CKDT_ALL;
     41                  if (ckdt)
     42                  {
     43                      // find first available active port
     44                      while (!(ckdt & 0x1))
     45                      {
     46                          port <<= 1;
     47                          ckdt >>= 1;
     48                      }
     49                      // force port selection in digital
     50                      SiiRegWrite(REG_PAUTH_RPOVR, 0x80 | port);
     51                      // ... and analog domains
     52                      SiiRegWrite(REG_PAUTH_RP_AOVR, port);
     53                  }
     54                  else
     55                  {
     56                      // if no input connection has been detected,
     57                      // resort to XCLK option
     58                      clockSrc = SI_TPG_CLK_XCLK;
     59                      DEBUG_PRINT( MSG_ERR, "Using APLL1 requires at least one active HDMI input!\n");
     60                  }
     61              }
     62              else
     63              {
     64                  // clear forced port selection
     65                  SiiRegWrite(REG_PAUTH_RPOVR, 0);
     66                  SiiRegWrite(REG_PAUTH_RP_AOVR, 0);
     67              }
     68          
     69              SiiRegModify( REG_VPG_CTRL_3, MSK_VPG_FORMAT | MSK_VPG_CLK_SEL, (clockSrc << 1) | (format << VAL_VPG_FORMAT_SHIFT));
     70          }
