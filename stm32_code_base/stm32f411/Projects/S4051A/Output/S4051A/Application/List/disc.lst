     39          /* ****************************************************************************  
     40           * void disc_initDisc(Disc *disc,hcInterface* source)
     41           * Description: This initialises the disc by loading the MBR and setting the
     42           * pointer to the hardware object.
     43          */
     44          void disc_initDisc(Disc *disc,IOManager* ioman)
     45          {
     46          	disc->ioman=ioman;
     47          	disc_setError(disc,DISC_NOERROR);
     48          	disc_loadMBR(disc);
     49          }
     50          /*****************************************************************************/ 
     51          
     52          /* ****************************************************************************  
     53           * void disc_loadMBR(Disc *disc)
     54           * Description: This functions copies the partitiontable to the partitions field.
     55          */
     56          void disc_loadMBR(Disc *disc)
     57          {
     58          	euint8 x;
     59          	euint8 *buf;
     60          	
     61          	buf=ioman_getSector(disc->ioman,LBA_ADDR_MBR,IOM_MODE_READONLY|IOM_MODE_EXP_REQ);
     62          	for(x=0;x<4;x++){
     63          		ex_getPartitionField(buf,&(disc->partitions[x]),PARTITION_TABLE_OFFSET+(x*SIZE_PARTITION_FIELD));
     64          	}
     65          	ioman_releaseSector(disc->ioman,buf);
     66          }
