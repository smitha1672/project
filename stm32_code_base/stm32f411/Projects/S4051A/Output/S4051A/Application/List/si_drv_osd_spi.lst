     18          //-------------------------------------------------------------------------------------------------
     19          //! @brief      Load OSD RAM from SPI directly.
     20          //! @param[in]  spiByteAddr     - Starting byte address within SPI flash memory
     21          //! @param[in]  osdWordAddr     - Starting OSD WORD address within OSD RAM
     22          //! @param[in]  spiByteLength   - Length in bytes of SPI data.  Padded to a multiple of OSD
     23          //!                               RAM words (6 bytes/word).
     24          //! @param[in]  showOptions
     25          //! @return     true - success, false - fail
     26          //-------------------------------------------------------------------------------------------------
     27          bool_t SiiDrvOsdSpiOsdRamLoad ( uint32_t spiByteAddr,  uint32_t osdWordAddr, uint16_t spiByteLength)
     28          {	
     29              SiiTimer_t  timerInfo;
     30          	uint16_t    osdWordLength;
     31          	int         mod;
     32          	bool_t      success;
     33          
     34              pDrvOsd->lastResultCode = SII_OSDDRV_SUCCESS;
     35          
     36          //    DEBUG_PRINT(MSG_DBG, "OSD RAM LOAD: SPI: %04X, OSD RAM start: %04X, SPI byte length: %d status = %02X\n", spiByteAddr, osdWordAddr, spiByteLength, mod );
     37              SiiOsTimerSet( &timerInfo, 0 );
     38          
     39          	// Load SPI start address
     40          	SiiRegWrite(REG_SPI_ADDR_LO, spiByteAddr & 0xFF);
     41          	SiiRegWrite(REG_SPI_ADDR_MID, (spiByteAddr >> 8) & 0xFF);
     42          	SiiRegWrite(REG_SPI_ADDR_HI, (spiByteAddr >> 16) & 0xFF);
     43          
     44          	// Determine padding for non-multiple of OSD_RAM_BURST_SIZE
     45          	mod = spiByteLength % OSD_RAM_BURST_SIZE;
     46          	if (mod)
     47          	{
     48          	    spiByteLength += (OSD_RAM_BURST_SIZE - mod);
     49          	}
     50          	osdWordLength = (spiByteLength / OSD_RAM_BURST_SIZE);
     51          
     52          	// Load SPI length - 1
     53          	spiByteLength -= 1;
     54          	SiiRegWrite(REG_SPI_LENGTH_LO, (spiByteLength) & 0xFF);
     55          	SiiRegWrite(REG_SPI_LENGTH_HI, (spiByteLength >> 0x08) & 0x3F);
     56          
     57          	// Load OSD start address and length
     58          	SiiRegWrite(REG_OSD_RAM_ADDR_L, osdWordAddr & 0xFF);
     59          	SiiRegWrite(REG_OSD_RAM_ADDR_H, (osdWordAddr >> 0x08) & 0x07);
     60          	SiiRegWrite(REG_OSD_RAM_WORD_COUNT_L, osdWordLength & 0xFF);
     61          	SiiRegWrite(REG_OSD_RAM_WORD_COUNT_H, (osdWordLength >> 0x08) & 0x0F);
     62          
     63          	// Start OSD read from SPI operation
