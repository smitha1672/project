     32          static uint16 ButtonLowLevel_get_ADC_Value( uint8 buttonID )
     33          {
     34              switch( buttonID )
     35              {
     36                  case 1:
     37                  case 2:
     38                  case 3:
     39                  case 4:
     40                  case 5:
     41                  case 6:
     42                  case 7:
     43                  case 8: 
     44                  case 9: 
     45                  case 10:
     46                  case 11:
     47                  {
     48                      adc_value = (ADC_GetConversionValue(ADC2)/10);
     49                  }
     50                      break;
     51          
     52                  case 0:
     53                  {
     54                      adc_value = (ADC_GetConversionValue(ADC1)/10);
     55                  }
     56                      break;
     57          
     58              }
     59              return adc_value;
     60              
     61          }
